{
  "expo": {
    "name": "my-player",
    "slug": "my-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "my-player",
    "platforms": ["ios", "android","web"],
    "permissions": ["CAMERA", "CAMERA_ROLL", "NOTIFICATIONS","ACCESS_COARSE_LOCATION",
    "ACCESS_FINE_LOCATION"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.yourapp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "ACCESS_COARSE_LOCATION",
          "ACCESS_FINE_LOCATION",
          "NOTIFICATIONS"
        ]
      },
      "package": "com.vieirajjr.myplayer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location"
     ],
     
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c2db0bd1-0d3c-4f1c-bb9e-98770fcbeead"
      }
    }
  }
}
